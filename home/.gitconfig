
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
	ui = true
[core]
	excludesfile = ~/.gitignore_global
	# editor problems with fish shell:
	# vim -X = don't look for X server, which can be slow
	editor = vim -X
[help]
	autocorrect = 1
[include]
	path = ~/.gitconfig.local
[alias]
	tpush = push
	tpull = pull
	pus = push
	pul = pull
[diff]
	tool = vimdiff
[merge]
	tool = vimdiff
[push]
	default = current
[pull]
	default = current
[branch]
	autosetuprebase = always
[user]
	name = Callan Bryant
	email = callan.bryant@gmail.com
[github]
	user = naggie
